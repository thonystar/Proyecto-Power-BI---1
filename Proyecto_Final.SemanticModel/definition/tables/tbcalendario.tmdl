	partition tbcalendario = calculated
		mode: import
		source = ```
				
				    var fecha_max = MAX(Fact_Sales[Date])
				    var fecha_min = MIN(Fact_Sales[Date])
				RETURN
				ADDCOLUMNS(
				    SELECTCOLUMNS(
				        CALENDAR(fecha_min,fecha_max),
				        "Fecha", [Date]
				    ),
				    "Año", YEAR([Fecha]),
				    "Mes", MONTH([Fecha]),
				    "Dia", DAY([Fecha]),
				    "NomMes", FORMAT([Fecha],"MMMM"),
				    "MesCorto", FORMAT([Fecha],"MMM"),
				    "Periodo", FORMAT([Fecha],"YYYYMM"),
				    "Dia_Sem", FORMAT([Fecha],"DDDD"),
				    "Dia_Sem_corto", UPPER(LEFT(FORMAT([Fecha],"dddd"),1)),
				    "Dia_Sem_Num", WEEKDAY([Fecha],2),
				    "Trimestre", "T" & QUARTER([Fecha]),
				    "NomMes_corto",
				    SWITCH(MONTH([Fecha]),
				        1,"E",
				        2,"F",
				        3,"M" & UNICHAR(8203),   -- marzo
				        4,"A" & UNICHAR(8203),   -- abril
				        5,"M" & UNICHAR(8204),   -- mayo
				        6,"J" & UNICHAR(8203),   -- junio
				        7,"J" & UNICHAR(8204),   -- julio
				        8,"A" & UNICHAR(8204),   -- agosto
				        9,"S",
				        10,"O",
				        11,"N",
				        12,"D"
				    )
				)
				```

